<h1>Edit List</h1>

<form action="/list/{{oneList._id}}?_method=PUT" method="POST">
    <label>Title</label>
    <input type="text" name="title" value="{{oneList.title}}" />

    <label>Produce</label>
    <input type= "text" name= "{{oneList.produce}}" />

    <label>Animal Protein</label>
    <textarea>{{oneList.animalProtein}}</textarea>

    <label>Dairy</label>
    <textarea>{{oneList.dairy}}</textarea>

    <input type="submit" value="Save" />
</form>